{
    "name": "rgtech/kcg-app",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "app/"
        ],
        "files": [
            "app/Helpers/functions.php",
            "app/Helpers/routes.php"
        ]
    },
    "autoload-dev": {
