            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Rafael Gomes"
        }
    ],
    "require": {
        "vlucas/phpdotenv": "^5.6",
        "symfony/http-foundation": "^7.2",
        "symfony/dependency-injection": "^7.2",
        "symfony/routing": "^7.2",
        "symfony/http-kernel": "^7.2",
        "doctrine/orm": "^3.3",
        "symfony/cache": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0"
    }
}
